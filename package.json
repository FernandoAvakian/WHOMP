    "react-prerender": "0.0.3",
    "requirejs": "^2.3.3",
    "style-loader": "^0.19.0",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  }
