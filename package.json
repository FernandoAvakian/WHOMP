{
  "name": "forest_atlas_template",
  "version": "1.4.2",
  "description": "Template for the WRI Forest Atlas that will be available through ArcGIS Online.",
  "main": "index.html",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.development.js",
    "build": "webpack --config webpack.production.js --display-origins --display-provided-exports --display-reasons",
    "clean": "rm -rf node_modules; rm package-lock.json",
    "profile": "webpack --profile --json > stats.json",
    "serve": "webpack-dev-server --mode production --open --https --compress",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wri/forest_atlas_template.git"
  },
  "keywords": [
    "wri",
    "arcgis",
    "agol",
    "esri",
    "javascript"
  ],
  "author": "BlueRaster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wri/forest_atlas_template/issues"
  },
  "homepage": "https://github.com/wri/forest_atlas_template#readme",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@arcgis/webpack-plugin": "^4.14.0",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.1",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4",
    "webpack": "^4.20.1",
    "webpack-cli": "~3.3.5",
    "webpack-dev-server": "^3.1.10",
    "webpack-hasjs-plugin": "^1.0.3",
    "webpack-merge": "^4.2.2"
  },
  "arcgis": {
    "type": "jsapi"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --bail"
    }
  }
}
